library(mgcv)
library(lubridate)
setwd('/Users/Evan/DataScience/R-Programming/HFMD/')
hfmd=read.csv('hfmd.csv')
hfmd$log=log(hfmd$cases)
hfmd$DOW=as.factor(hfmd$DOW)
hfmd$holiday=as.factor(hfmd$holiday)
hfmd$day=as.factor(hfmd$day)
hfmd$time<-1:length(hfmd[,1])
fit1<-gam(cases~s(time,k=8)+s(ave_temp,k=5)+DOW+holiday,family=poisson,method="REML",data=hfmd)
plot(fit1)
fit1<-gam(cases~s(time,k=6)+s(ave_temp,k=5)+DOW+holiday,family=poisson,method="REML",data=hfmd)
plot(fit1)
